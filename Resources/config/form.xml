<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="f_devs_tag.form_type_tag.class">FDevs\TagBundle\Form\Type\TagType</parameter>
        <parameter key="f_devs_tag.model_tag.class">FDevs\TagBundle\Model\Tag</parameter>
    </parameters>

    <services>
        <service id="f_devs_tag.form_type_tag" class="%f_devs_tag.form_type_tag.class%">
            <tag name="form.type" alias="fdevs_tag"/>
            <argument>%f_devs_tag.list_type%</argument>
        </service>
    </services>

</container>
